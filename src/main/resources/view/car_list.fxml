<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="formNode" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CarListController">
   <children>
      <Label layoutX="252.0" layoutY="28.0" text="Car List">
         <font>
            <Font name="System Bold" size="28.0" />
         </font>
      </Label>
      <TableView fx:id="carTable" layoutX="71.0" layoutY="91.0" onMouseClicked="#carTableOnMouseClicked" prefHeight="241.0" prefWidth="468.0">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
      <Button layoutX="71.0" layoutY="347.0" mnemonicParsing="false" onAction="#newBtnOnAction" text="New" />
      <Button layoutX="190.0" layoutY="347.0" mnemonicParsing="false" onAction="#printBtnOnAction" text="Print" />
      <Button fx:id="updateBtn" layoutX="371.0" layoutY="347.0" mnemonicParsing="false" onAction="#updateBtnOnAction" text="Update" />
      <Button fx:id="deleteBtn" layoutX="487.0" layoutY="347.0" mnemonicParsing="false" onAction="#deleteBtnOnAction" text="Delete" />
   </children>
</AnchorPane>
